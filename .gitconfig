[core]
	excludesfile = /Users/mei/.gitignore_global
	editor = nvim
[diff]
  tool = nvimdiff
[difftool "nvimdiff"]
  cmd = "nvim -R -d -c \"wincmd l\" -d \"$LOCAL\" \"$REMOTE\""
[mergetool "nvimdiff"]
  cmd = "nvim -d -c \"4wincmd w | wincmd J\" \"$LOCAL\" \"$BASE\" \"$REMOTE\"  \"$MERGED\""
[user]
	name = mei
	email = mei28aquarius@gmail.com
[commit]
	template = /Users/mei/.stCommitMsg
[init]
	defaultBranch = main
[mergetool]
	keepBackup = false
[credential]
	helper = store
